@model CalculateViewModel

<br />
<br />
<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">@Model._Student.Nume</h3>
    </div>
    <div class="panel-body">
        <div class="col-md-6">
            <h4>Matricol</h4>
            <p>@Model._Student.Matricol</p>
        </div>
        <div class="col-md-6">
            <h4>Media</h4>
            <p>@Model._Student.media</p>
        </div>
        <h3>Result: @Model.result</h3>
    </div>

    <div class="panel-footer">
        <a class="btn btn-primary" asp-action="Index">Home</a>
    </div>
</div>